{
  "name": "typescript-orm-benchmark",
  "version": "0.1.0",
  "description": "Example base for typescript orm benchmark",
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit && eslint \"*/**/*.ts\"",
    "lint-diff": "git diff --name-only --cached --relative | grep \\\\.ts$ | xargs eslint",
    "postinstall": "npm run build",
    "precommit": "npm run lint-diff",
    "knex": "node -r dotenv/config ./node_modules/.bin/knex",
    "migrate:make": "knex migrate:make --knexfile ./src/knex/config.ts",
    "truncate": "knex migrate:rollback --all --knexfile ./src/knex/config.ts",
    "posttruncate": "knex migrate:latest --knexfile ./src/knex/config.ts",
    "migrate": "knex migrate:latest --knexfile ./src/knex/config.ts",
    "start": "ts-node-dev --respawn --transpile-only ./server.ts"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "dist/server.js",
  "author": "Emanuel Casco",
  "license": "MIT",
  "dependencies": {
    "@codenotary/immudb-node": "^2.0.0-alpha.1",
    "@prisma/client": "^5.5.2",
    "autocannon": "^7.12.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "knex": "^3.0.1",
    "knex-stringcase": "^1.4.6",
    "objection": "^3.1.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.33.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/babel-core": "^6.25.9",
    "@types/babel-traverse": "^6.25.9",
    "@types/bluebird": "^3.5.41",
    "@types/body-parser": "^1.19.4",
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/sequelize": "^4.28.17",
    "@types/validator": "^13.11.5",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/eslint-plugin-tslint": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel": "6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-preset-env": "^1.7.0",
    "dictum.js": "^1.0.0",
    "eslint": "^8.52.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "prisma": "^5.5.2",
    "sequelize-cli": "^6.6.1",
    "ts-node-dev": "1.0.0-pre.40",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2"
  }
}
