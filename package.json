{
  "name": "typescript-orm-benchmark",
  "version": "0.1.0",
  "description": "Example base for typescript orm benchmark",
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit && eslint \"*/**/*.ts\"",
    "lint-diff": "git diff --name-only --cached --relative | grep \\\\.ts$ | xargs eslint",
    "postinstall": "npm run build",
    "precommit": "npm run lint-diff",
    "knex": "node -r dotenv/config ./node_modules/.bin/knex",
    "migrate:make": "knex migrate:make --knexfile ./src/knex/config.ts",
    "truncate": "knex migrate:rollback --all --knexfile ./src/knex/config.ts",
    "posttruncate": "knex migrate:latest --knexfile ./src/knex/config.ts",
    "migrate": "knex migrate:latest --knexfile ./src/knex/config.ts",
    "start": "ts-node-dev --respawn --transpile-only ./server.ts"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "main": "dist/server.js",
  "author": "Emanuel Casco",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "autocannon": "^7.10.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^2.3.0",
    "knex-stringcase": "^1.4.6",
    "objection": "^3.0.1",
    "pg": "^8.8.0",
    "prisma": "^4.4.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.25.2",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@types/babel-core": "^6.25.7",
    "@types/babel-traverse": "^6.25.7",
    "@types/bluebird": "^3.5.37",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.0",
    "@types/pg": "^8.6.5",
    "@types/sequelize": "^4.28.14",
    "@types/validator": "^13.7.8",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel": "6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.2.0",
    "babel-preset-env": "^1.7.0",
    "dictum.js": "^1.0.0",
    "eslint": "^8.25.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "sequelize-cli": "^6.5.1",
    "ts-node-dev": "1.0.0-pre.40",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  }
}
